<form ng-submit="signIn()" >
  <ul class="errors">
    <li ng-repeat="error in errors">{{error}}</li>
  </ul>
  <p>
    <%= label_tag :email %>
    <br>
    <%= text_field_tag :email, '', 'ng-model' => 'email' %>
  </p>
  <p>
    <%= label_tag :password %>
    <br>
    <%= password_field_tag :password, '', 'ng-model' => 'password' %>
  </p>
  <p>
    <%= submit_tag 'Sign in' %>
    <a href="/sign_up">Sign up</a>
  </p>
</form>
